	<build>
		<plugins>
			<plugin>
				<!--
				Override maven-enforcer-plugin from owsi-core-parent-maven-configuration-common
				so that we can enforce dependencies differently in OWSI-Core.
				(Note that owsi-core-parent-maven-configuration-common is also used independently from OWSI-Core)
				-->
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-enforcer-plugin</artifactId>
				<version>${owsi-core.maven-enforcer-plugin.version}</version>
				<executions>
					<execution>
						<id>enforce</id>
						<goals>
							<goal>enforce</goal>
						</goals>
						<configuration>
							<rules>
								<DependencyConvergence />
								<bannedDependencies>
									<excludes>
										<exclude>org.springframework:spring</exclude>
										<exclude>org.springframework:spring-ldap</exclude>
										<exclude>commons-logging:commons-logging</exclude>
										<exclude>cglib:cglib</exclude>
										<exclude>rhino:rhino</exclude>
										<exclude>rhino:js</exclude>
										<exclude>javassist:javassist</exclude>
										<exclude>apache-xerces:xml-apis</exclude>
										<exclude>javax.servlet:servlet-api</exclude>
										<exclude>net.sf.ehcache:ehcache-core</exclude>
										<exclude>postgresql:postgresql</exclude>
									</excludes>
								</bannedDependencies>
								<requireMavenVersion>
									<version>[${minimal.maven.version},)</version>
								</requireMavenVersion>
								<requireJavaVersion>
									<version>${jdk.version}</version>
								</requireJavaVersion>
							</rules>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>fr.openwide.maven.plugins</groupId>
					<artifactId>localization-properties-consistency-maven-plugin</artifactId>
					<version>${owsi-core.localization-properties-consistency-maven-plugin.version}</version>
					<executions>
						<execution>
							<id>scan</id>
							<goals>
								<goal>scan</goal>
							</goals>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>org.bsc.maven</groupId>
					<artifactId>maven-processor-plugin</artifactId>
					<dependencies>
						<dependency>
							<groupId>org.bindgen</groupId>
							<artifactId>bindgen</artifactId>
							<version>${owsi-core.bindgen.version}</version>
						</dependency>
						<dependency>
							<groupId>com.querydsl</groupId>
							<artifactId>querydsl-apt</artifactId>
							<classifier>jpa</classifier>
							<version>${owsi-core.querydsl.version}</version>
						</dependency>
					</dependencies>
					<configuration>
						<processors>
							<processor>com.querydsl.apt.jpa.JPAAnnotationProcessor</processor>
							<processor>org.bindgen.processor.Processor</processor>
						</processors>
					</configuration>
					<executions>
						<execution>
							<id>maven-processor-plugin-default</id>
							<phase>generate-sources</phase>
							<goals>
								<goal>process</goal>
							</goals>
						</execution>
						<execution>
							<id>maven-processor-plugin-default-test</id>
							<phase>generate-test-sources</phase>
							<goals>
								<goal>process-test</goal>
							</goals>
						</execution>
					</executions>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
	
	<profiles>
		<profile>
			<id>eclipse-build</id>
			<!--
			An automatically-set profile that disables proccessors' sources generation.
			They are still ran in order to automatically set the generated sourcepaths on the eclipse projects. This is
			done at the same time for both main sources and test sources, presumably because doing otherwise upsets Eclipse.
			Detail: the processors are generally too resource-consuming to be ran as part of
			the automatic Eclipse build, which is triggered by each file change.
			-->
			<activation>
				<property>
					<name>m2e.version</name>
				</property>
			</activation>
			<build>
				<pluginManagement>
					<plugins>
						<plugin>
							<groupId>org.codehaus.mojo</groupId>
							<artifactId>animal-sniffer-maven-plugin</artifactId>
							<version>${owsi-core.animal-sniffer-maven-plugin.version}</version>
							<executions>
								<execution>
									<id>check-java-version</id>
									<phase>compile</phase>
									<goals>
										<goal>check</goal>
									</goals>
									<configuration>
										<ignores>
											<!-- Test bindings are generated in the main code, but they reference
											test classes that are absent from the main code, which makes animal-sniffer
											cry. We tell him not to mind about that.
											-->)
											<ignore>fr.openwide.core.test.*</ignore>
										</ignores>
									</configuration>
								</execution>
							</executions>
						</plugin>
					</plugins>
				</pluginManagement>
			</build>
		</profile>
		<profile>
			<id>eclipse-processor</id>
			<!--
			A profile for sources generation. Simply maps the maven-processor-plugin executions to the generate-sources
			phase (even for test sources), so that we only need to run generate-sources and nothing else.
			-->
			<properties>
				<generated-sources-test.directory>${generated-sources.directory}</generated-sources-test.directory>
			</properties>
		</profile>
		<profile>
			<id>jdk7</id>
			<activation>
				<property>
					<name>jdk7</name>
				</property>
			</activation>
			<properties>
				<!-- override bindgen-java version to handle jdk7 compatible build -->
				<owsi-core.bindgen-java.version>${owsi-core.bindgen-java-jdk7.version}</owsi-core.bindgen-java.version>
			</properties>
		</profile>
	</profiles>
	
